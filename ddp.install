<?php/** * Implementation of hook_schema(). * @return array */function ddp_schema() {  $schema = array();  $schema['ddp_foodgroup'] = array('description' => 'Table with food groups', );  $schema['ddp_foodgroup']['fields']['fgid'] = array(    'description' => 'Food Group ID',    'type' => 'serial',    'unsigned' => TRUE,    'not null' => TRUE,  );  $schema['ddp_foodgroup']['fields']['name'] = array(    'description' => 'Food Group',    'type' => 'varchar',    'not null' => TRUE,    'length' => 10,  );  $schema['ddp_foodgroup']['indexes'] = array(      'Food Groups' => array('fgid'),  );  watchdog('DEBUG', 'Passou pelo schema');  return $schema;}